{
  "expo": {
    "name": "seekingSame",
    "slug": "seekingSame",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/newlogo.png",
    "scheme": "seekingsame",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyAy3lwlcebC9YJs_EjObxllyhJovtDfRA0"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/androidicon.jpeg"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAy3lwlcebC9YJs_EjObxllyhJovtDfRA0"
        }
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/newlogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/newlogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
